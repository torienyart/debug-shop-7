<%= link_to "Items", '/items' %>
<%= link_to "Merchants", '/merchants' %>

<h2>Edit <%=link_to @merchant.name, "/merchants/#{@merchant.id}" %></h2>
<%= form_for @merchant, url: "/merchants/#{@merchant.id}", method: :patch do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name%>

  <%= form.label :address %>
  <%= form.text_field :address%>

  <%= form.label :city %>
  <%= form.text_field :city%>

  <%= form.label :state %>
  <%= form.text_field :state%>

  <%= form.label :zip %>
  <%= form.text_field :zip%>

  <%= form.submit 'Update Merchant' %>
<% end %>
