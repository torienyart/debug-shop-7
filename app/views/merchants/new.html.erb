<%= link_to "Items", '/items' %>
<%= link_to "Merchants", '/merchants' %>
<%= form_for @merchant, url: '/merchants', method: :create do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :city %>
  <%= form.text_field :city %>

  <%= form.label :state %>
  <%= form.text_field :state %>

  <%= form.label :zip %>
  <%= form.text_field :zip %>

  <%= form.submit 'Create Merchant' %>
<% end %>
